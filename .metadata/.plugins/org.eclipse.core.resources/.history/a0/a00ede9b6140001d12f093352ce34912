package selenium;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;

public class ShoppingForDress {


	public static void main (String[] args) throws InterruptedException {
		System.setProperty("webdriver.chrome.driver","C:\\Deepak\\Parul_Learning\\SDET81\\Assignment\\Maven_1\\src\\test\\resources\\drivers\\chromedriver.exe");
		WebDriver driver = new ChromeDriver();
		driver.get(" http://automationpractice.com/index.php");


		driver.manage().window().maximize();

	

	WebElement search =driver.findElement( By.xpath("//input[@id='search_query_top']"));
	search.click();
	search.sendKeys("summerdress");
	
	

	WebElement searchsummerdress =driver.findElement( By.xpath("//div[@id='search_block_top']/form/button"));
	 searchsummerdress.click();
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}
}
